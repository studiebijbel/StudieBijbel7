	{
	//	plugins.dialogs.showErrorDialog('', text.substring(indexes[k], indexes[k]+lengths[k]))
		var regEx2 = '[A-Za-z0-9]';
		var wasMatched = new RegExp(regEx2, "g" );
		
		var shouldmatch = wasMatched.exec(text.substring((indexes[k])-1, (indexes[k])));	
