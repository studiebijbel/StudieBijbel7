			}
		} else {
			iDoeMin = 0;
		}
		
		
	
		text = text.substring(0, (indexes[k]) - iDoeMin) + newTexts[k] + text.substring((indexes[k] - iDoeMin)+(lengths[k] + iDoeMin))
	}
}

return text

